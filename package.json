{
  "name": "vue-image-magnifier",
  "version": "0.2.1",
  "private": false,
  "main": "lib/magnifier.umd.js",
  "scripts": {
    "demo": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name magnifier --dest lib src/main.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "vue",
    "imagnifier",
    "image",
    "zoom"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/anthinkingcoder/vue-image-magnifier.git"
  },
  "license": "MIT"
}
